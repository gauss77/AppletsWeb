        		<file name="org/eclipse/swt/internal/dnd/DragAndDrop.js"/>
        		<file name="org/eclipse/swt/internal/dnd/DNDUtils.js"/>
        		<file name="org/eclipse/swt/internal/dnd/ShellFrameDND.js"/>
